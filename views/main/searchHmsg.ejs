<!DOCTYPE html>
<html>
<head>
  <title>Index Page</title>
  <!-- css link -->
  <link rel="stylesheet" href="/css/History.css">
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
  <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.4.1/semantic.min.css">
  <!-- fontowsome-->
  <script src="https://kit.fontawesome.com/906e9173fa.js" crossorigin="anonymous"></script>

</head>
<body>

  
  <!-- Navigation -->
  
  <nav class="navbar navbar-expand-lg sticky-top ">
    <a class="navbar-brand" href="#"></a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <!-- left side-->
      <ul class=" left_nav navbar-nav">
        <li class="nav-item active">
          <a class="nav-link" href="/main">HOME <span class="sr-only">(current)</span></a>
        </li>
      </ul>
      <!--Right side-->
      <ul class="navbar-nav right_nav ml-auto">
        <li class="nav-item active">
          <a class="nav-link" href="/logout">LOGOUT<span class="sr-only"></span></a>
        </li>
      </ul>
    </div>
  </nav>

  
  
  <!-- Main-Body -->
  <div class="container-fluid mt-5 mb-5">
    <div class="row">
    
      <!--left sidebar-->
        <% if(messages.length == 0) {%>
            <div class="container">
              <div class="alert alert-danger" role="alert">
                <strong><%=error%></strong>
              </div>
            </div>
        <%}%>

        <div class="container mt-5 mb-5">
    <div class="row">
    

<div class="container searchh">
  <form class="form-inline ml-auto mr-auto"  action="/main/searchH/msgH" method="get" >
    <div class="form-group mx-sm-3 mb-2">
      <div class="ui selection dropdown">
            <input type="hidden" name="type" required>
            <i class="dropdown icon"></i>
            <div class="default text">msg type</div>
            <div class="menu">
              <div class="item" data-value="ERROR">ERROR</div>
              <div class="item" data-value="INFO">INFO</div>
            </div>
          </div>
      </div>
    <div class="form-group mx-sm-3 mb-2">
      <input class="form-control" type="text" name="code" placeholder="msg code">
    </div>
    <div class="form-group mx-sm-3 mb-2">
      <input class="form-control" type="text" name="name" placeholder="msg name">
    </div>
    <button type="submit" class="button mb-2 ui teal">Search</button>
  </form>
</div>

      <!--main body-->
      <div class="container mt-5">
    
       <h1 style="text-align: center">Message Records</h1>
       <table class="ui celled fixed striped black table mt-5">
          <thead>
           <tr S.No.>
           <th class="one wide">S.No.</th>
           <th>Msg type</th>
           <th>Msg name</th>
           <th>Msg code</th>
           <th>Created By</th>
           <th>Date</th>
          </tr>
          </thead>
          <tbody>
        <% i = 1%>
        <% messages.reverse().forEach(function(msg){ %>
           <tr>
            <td data-label="S.No."><%= i %></td>
            <td data-label="msg type"><%= msg.type %></td>
            <td data-label="msg name"><%= msg.name %></td>
            <td data-label="msg code"><%= msg.code %></td>
            <td data-label="Created By"><%= msg.author.username %></td>
            <td data-label="date"><%= msg.date %></td>
           </tr>
            <% i = i+1 %>
        <% }); %> 
        </tbody>
      </table>





      <!--right sidebar-->
    </div>



    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.4.1/semantic.min.js"></script>
     <script>
    $('.ui.dropdown')
       .dropdown();
  </script>

  </body>
  </html>













